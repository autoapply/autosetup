    "pkginfo": "^0.4.1",
    "tmp-promise": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "ejs-lint": "^0.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-flowtype": "^3.9.1",
    "flow-bin": "^0.100.0",
    "mocha": "^6.1.4",
    "svg-term-cli": "^2.1.1"
