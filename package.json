    "watch": "babel --watch src/ -d dist/",
    "prepublish": "yarn run build"
  },
  "dependencies": {
    "argparse": "^1.0.9",
    "deepmerge": "^3.2.0",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1",
    "js-yaml": "^3.12.2",
    "pkginfo": "^0.4.1",
    "tmp-promise": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-flow": "^6.23.0",
    "chai": "^4.2.0",
    "ejs-lint": "^0.3.0",
    "eslint": "^5.15.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "flow-bin": "^0.96.0",
    "mocha": "^6.0.2",
    "svg-term-cli": "^2.1.1"
