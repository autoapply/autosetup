{
  "name": "autosetup",
  "version": "0.3.3",
  "description": "Tool to install autoapply in a Kubernetes cluster",
  "license": "MIT",
  "author": "Pascal",
  "homepage": "https://github.com/autoapply/autosetup",
  "repository": {
    "type": "git",
    "url": "https://github.com/autoapply/autosetup.git"
  },
  "bin": {
    "autosetup": "./dist/autosetup.js"
  },
  "files": [
    "dist",
    "src",
    "templates",
    "example-config.yaml"
  ],
  "scripts": {
